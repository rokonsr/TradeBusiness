body {
    margin: 0px;
}

#bg {
    background-image: url("/Theme/img/bg-img/clover-pos.jpg");
    background-size: cover;
    background-position:center center;
    position: relative;
    top:0;
    bottom:0;
    height:100vh;
    z-index:-10;
}

#bg-overlay {
    position: absolute;
    background-color: rgba(0,0,0,0.5);
    top:0;
    bottom:0;
    height:100%;
    width:100%;
    z-index:-5;
        
}
